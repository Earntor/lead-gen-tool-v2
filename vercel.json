{
  "headers": [
    {
      "source": "/api/track",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow, nosnippet, noarchive" },
        { "key": "Cache-Control", "value": "no-store" }
      ]
    },
    {
      "source": "/api/ingest-token",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow, nosnippet, noarchive" },
        { "key": "Cache-Control", "value": "no-store" }
      ]
    },
    {
      "source": "/tracker.js",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow, nosnippet, noarchive" },
        { "key": "Cache-Control", "value": "public, max-age=3600, stale-while-revalidate=86400" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://www.google.com https://recaptcha.google.com;"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/worker-enrich",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/digest?frequency=daily&secret=725af37e0d8d87341be4d2d087407a1433195bffe00998a12aa95e66842e93b1",
      "schedule": "30 5 * * *"
    },
    {
      "path": "/api/cron/digest?frequency=weekly&secret=725af37e0d8d87341be4d2d087407a1433195bffe00998a12aa95e66842e93b1",
      "schedule": "30 5 * * 1"
    },
    {
      "path": "/api/cron/digest?frequency=monthly&secret=725af37e0d8d87341be4d2d087407a1433195bffe00998a12aa95e66842e93b1",
      "schedule": "30 5 1 * *"
    }
  ]
}
